
@model  Yunan.Models.ScenicCollectionViewModel
@{
    Layout = null;
}
<ul class="bod-mian-ul">
    @foreach (var item in Model.ScenicType)
    {
        <li>
            <div class="main-ul-div1" style="background-image:url(../../@item.CoverImg)">
                <div class="main-ul-div2">
                    <p class="p3">￥@Html.ActionLink(@item.price.ToString(),"ScenicDetail","Home",new {id=@item.ScenicId },new{@class="aclink1" })</p>
                </div>
            </div>
            <div class="main-ul-div3">
                <p>@item.ScenicTitle</p>
                <p style="color:#999">彩云价格：<span class="span1">@item.price</span></p>
                <p>描述：<span style="color:#999">@item.ScenicKeyWords</span></p>
            </div>
        </li>
    }

</ul>               